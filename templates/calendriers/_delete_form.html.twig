<form method="post" action="{{ path('calendriers_delete', {'id': calendrier.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer cet évènement ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ calendrier.id) }}">
    <button class="btn far fa-trash-alt"></button>
</form>
