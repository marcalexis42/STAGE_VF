<form method="post" action="{{ path('user_data_delete', {'id': user_datum.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user_datum.id) }}">
    <button class="btn">Supprimer</button>
</form>
